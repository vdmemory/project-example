    "scripts": {
        "prepare": "is-ci || husky install",
        "husky:uninstall": "npx rimraf .husky && git config --unset core.hooksPath",
        "husky:pre-commit": "husky add .husky/pre-commit \"yarn by-pre-commit\"",
        "husky:pre-push": "husky add .husky/pre-push \"yarn by-pre-push\"",
        "start": "nx serve",
        "build": "nx build",
        "test": "nx test",
        "lint": "nx workspace-lint && nx lint",
        "e2e": "nx e2e",
        "start:auth": "nx run auth:serve --port=4200 --max_old_space_size=8192",
        "start:client": "nx run client:serve --hostname=0.0.0.0 --port=4201 --max_old_space_size=8192",
        "start:agency": "nx run agency:serve --hostname=0.0.0.0 --port=4202 --max_old_space_size=8192",
        "start:apps": "yarn start:auth & yarn start:client & yarn start:agency",
        "start:curator": "nx run curator:serve --hostname=0.0.0.0 --port=4203",
        "build:auth": "nx run auth:export",
        "build:client": "nx run client:build",
        "build:agency": "nx run agency:build",
        "build:curator": "nx run curator:build",
        "aff:test": "nx affected:test",
        "aff:coverage": "nx affected:test --codeCoverage --skip-nx-cache",
        "aff:code-coverage": "nx affected:test --parallel --codeCoverage --coverageReporters=text-summary,lcov,cobertura",
        "code-coverage": "yarn nx run-many --codeCoverage --skip-nx-cache --coverageReporters=text-summary,lcov,cobertura --no-watch -t test",
        "aff:lint": "nx affected:lint",
        "aff:lint-no-cache": "nx affected:lint --skip-nx-cache",
        "aff:typecheck": "nx affected --target=typecheck",
        "format": "nx format:write",
        "format:check": "nx format:check",
        "clean:dist": "npx rimraf dist",
        "clean:tmp": "npx rimraf tmp",
        "clean:cache": "npx rimraf node_modules/.cache",
        "by-pre-commit": "yarn aff:lint && yarn format && git add . && yarn format:check",
        "by-pre-push": "yarn aff:typecheck && yarn aff:test",
        "storybook": "nx run ui-kit:storybook"
    },
